{
  "name": "vaki",
  "version": "0.5.7",
  "description": "a simple terminal tool",
  "bin": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "npm run changelog && git add CHANGELOG.md",
    "lint": "eslint --fix src/",
    "preversion": "npm run changelog && git add CHANGELOG.md",
    "postversion": "npm publish && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/limengke123/van.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "terminal",
    "tool"
  ],
  "files": [
    "src"
  ],
  "author": "limengke123",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/limengke123/van/issues"
  },
  "homepage": "https://github.com/limengke123/van#readme",
  "dependencies": {
    "araneida": "^1.0.3",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^2.20.0",
    "configstore": "^4.0.0",
    "figlet": "^1.2.1",
    "iconv-lite": "^0.4.24",
    "inquirer": "^6.3.1",
    "opener": "^1.5.1",
    "semver": "^6.0.0",
    "terminal-link": "^1.3.0",
    "update-notifier": "^2.5.0"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "conventional-changelog-cli": "^2.0.17",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "ora": "^3.4.0"
  }
}
